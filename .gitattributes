# Repository canonical EOL: LF. Windows checkouts may be CRLF (via core.autocrlf=true).

*.md    text eol=lf
*.tex   text eol=lf
*.sty   text eol=lf
*.cls   text eol=lf
*.bib   text eol=lf
*.yml   text eol=lf
*.yaml  text eol=lf
*.json  text eol=lf
*.xml   text eol=lf
*.html  text eol=lf
*.js    text eol=lf
*.ts    text eol=lf
*.py    text eol=lf
*.sh    text eol=lf
*.csv   text eol=lf
*.ps1   text eol=crlf
*.bat   text eol=crlf
*.cmd   text eol=crlf

*.svg   text eol=lf

*.png   binary
*.jpg   binary
*.jpeg  binary
*.gif   binary
*.pdf   binary
*.zip   binary
*.tar.gz binary

*       text=auto
